{
    "name": "jostkleigrewe/alexa-core-bundle",
    "type": "symfony-bundle",
    "description": "AlexaCoreBundle for the Symfony Framework",
    "keywords": ["Jostkleigrewe", "Alexa", "Symfony", "Bundle"],
    "homepage": "https://projects.jostkleigrewe.com/jostkleigrewe/alexa-core-bundle/",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Jostkleigrewe",
            "email": "sven@jostkleigrewe.com"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.15",
        "symfony/config": "^6.3",
        "symfony/framework-bundle": "^6.3",
        "symfony/serializer": "^6.3"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "Jostkleigrewe\\AlexaCoreBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Jostkleigrewe\\AlexaCoreBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
        }
    },
    "require-dev": {
        "symfony/test-pack": "dev-main"
    },
    "scripts": {
        "phpunit": "php vendor/bin/phpunit",
        "phpunit-coverage": "XDEBUG_MODE=coverage php vendor/bin/phpunit --coverage-html=var/coverage"
    }
}
